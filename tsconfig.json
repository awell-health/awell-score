{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "noImplicitAny": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}